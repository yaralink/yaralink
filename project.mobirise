lor'\">\n\t\t<input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#149dcc\" condition=\"overlay && bg.type !== 'color'\">\n\t\t<input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.6\" condition=\"overlay && bg.type !== 'color'\">\n\t\t<!-- End block parameters -->\n\t</mbr-parameters>\n\n\t<div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n\t</div>\n\n\t<div class=\"container\">\n\t\t<div class=\"row\">\n\t\t\t<div class=\"col-lg-12 content__block align-center\">\n\t\t\t\t<h3 class=\"mbr-section-subtitle mbr-semibold mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-subtitle\"><b>Site Price</b></h3>\n\t\t\t\t<h2 class=\"mbr-section-title mbr-fonts-style\" mbr-theme-style=\"display-1\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title\"><b>Get Company Site in 5-15 days</b></h2>\n\t\t\t</div>\n\t\t</div>\n\t\t<div class=\"row wrap justify-content-center\">\n\t\t\t<div class=\"col-lg-4 col-md-6\">\n\t\t\t\t<div class=\"wrap__card item1\">\n\t\t\t\t\t<h4 class=\"card__title mbr-bold mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".card__title\">Company Site</h4>\n\t\t\t\t\t<h3 class=\"card__price mbr-bold mbr-fonts-style\" mbr-theme-style=\"display-1\" data-app-selector=\".card__price\">35500</h3>\n\t\t\t\t\t<div class=\"time__block\">\n\t\t\t\t\t\t<p class=\"card__time mbr-medium mbr-fonts-style\" mbr-if=\"showTime\" mbr-theme-style=\"display-4\" data-app-selector=\".card__time\">1 Static Website&nbsp;<br></p>\n\t\t\t\t\t</div>\n\t\t\t\t\t<p class=\"mbr-text mbr-medium mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".mbr-text, .mbr-section-btn, .price__list_check, .price__list_close, .time__block\">You will get your&nbsp;<b>Personal Blog</b> in <b>1 day</b>!</p>\n\t\t\t\