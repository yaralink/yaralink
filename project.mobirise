          "_customHTML": "<section data-bs-version=\"5.1\" class=\"footer1 mediam5\" group=\"Footers\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" global once=\"footers\" mbr-class=\"{\n    'mbr-parallax-background': bg.parallax}\">\n\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Main Links\" name=\"showMainLinks\" checked>\n        <input type=\"checkbox\" title=\"Links\" name=\"showLinks\" checked>\n        <input type=\"checkbox\" title=\"Contacts 1\" name=\"showContacts1\" checked>\n        <input type=\"checkbox\" title=\"Contacts 2\" name=\"showContacts2\" checked>\n        <input type=\"range\" inline title=\"Contacts 1 Count\" name=\"contacts1Count\" min=\"1\" max=\"2\" step=\"1\" value=\"2\" condition=\"showContacts1\">\n        <input type=\"range\" inline title=\"Contacts 2 Count\" name=\"contacts2Count\" min=\"1\" max=\"2\" step=\"1\" value=\"2\" condition=\"showContacts2\">\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked condition=\"showContacts1 || showContacts2\">\n        <input type=\"checkbox\" title=\"Icons\" name=\"showIcon\" checked condition=\"showContacts1 || showContacts2\">\n        <input type=\"color\" title=\"Icon Color\" name=\"iconColor\" value=\"#FACB1D\" selected condition=\"showIcon && (showContacts1 || showContacts2)\">\n        <input type=\"checkbox\" title=\"Text\" name=\"showText\" checked>\n        <input type=\"checkbox\" title=\"Copyright\" name=\"showCopyright\" checked>\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@ADDONS_PATH@/mediam5/components/_images/background3.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#1a2026\" selected>\n            <input type=\"video\" title=\"Video\" value=\"https://vimeo.com/428046504\">\n        </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"@ADDONS_PATH@/mediam5/components/_images/background2.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#181817\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\">\n            <div class=\"col-12\">\n                <div class=\"main-container\">\n                    <div class=\"main-links grid-box\" mbr-if=\"showMainLinks\">\n                        <div class=\"item-wrap\">\n                            <ul class=\"list main-list mbr-fonts-style\" mbr-theme-style=\"display-2\" data-app-selector=\".main-list\" data-multiline=\"true\" data-app-edit=\"full\" mbr-article>\n                                <li class=\"mbr-text item-wrap\" data-app-selector=\".main-list\"><a href=\"sites_type.html\" class=\"text-primary\">SITES</a></li>\n                                <li class=\"mbr-text item-wrap\" data-app-selector=\".main-list\"><a href=\"index.html#pricing-3d\" class=\"text-primary\">PRICES</a></li>\n                                <li class=\"mbr-text item-wrap\" data-app-selector=\".main-list\"><a href=\"yaralink_dev.html\" class=\"text-primary\">ABOUT</a></li>\n                                <li class=\"mbr-text item-wrap\" data-app-selector=\".main-list\"><a href=\"customer_support.html\" class=\"text-primary\">SUPPORT</a></li>\n                                <li class=\"mbr-text item-wrap\" data-app-selector=\".main-list\"><a href=\"faq.html\" class=\"text-primary\">FAQ</a></li>\n                            </ul>\n                        </div>\n                    </div>\n                    <div class=\"second-links grid-box\" mbr-if=\"showLinks\">\n                        <div class=\"item-wrap\">\n                            <ul class=\"list second-list mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".second-list\" data-multiline=\"true\" data-app-edit=\"full\" mbr-article>\n                                <li class=\"mbr-text item-wrap\" data-app-selector=\".second-list\"><a href=\"personal_blog.html\" class=\"text-primary\">Personal Blog</a></li>\n                                <li class=\"mbr-text item-wrap\" data-app-selector=\".second-list\"><a href=\"company_site.html\" class=\"text-primary\">Company Site</a></li>\n                                <li class=\"mbr-text item-wrap\" data-app-selector=\".second-list\"><a href=\"online_store.html\" class=\"text-primary\">Online Store</a></li>\n                                <li class=\"mbr-text item-wrap\" data-app-selector=\".second-list\"><a href=\"chat_bot.html\" class=\"text-primary\">Chat Bot</a></li>\n                                <li class=\"mbr-text item-wrap\" data-app-selector=\".second-list\"><a href=\"get_site.html\" class=\"text-primary\">Get Site Now</a></li>\n                            </ul>\n                        </div>\n                    </div>\n\n                    <div clas