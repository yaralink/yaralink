12 col-lg-{{textWidth}}\">\n                <div class=\"row {{contentAlign}} flex-grow-1\">\n\n                    \n\n                    <div class=\"item features-image col-12 col-md-6 col-lg-4 active\">\n                        <div class=\"item-wrapper\">\n                            <div class=\"card-box\" mbr-if=\"showTitle || showSubtitle\">\n                                <h5 class=\"card-title align-left mbr-fonts-style align-center\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".card-title\">\n                                    ART</h5>\n                                <h6 class=\"card-subtitle align-left mbr-fonts-style align-center\" mbr-theme-style=\"display-4\" mbr-if=\"showSubtitle\" data-app-selector=\".card-subtitle\"><b>\n                                    Head of Design</b><br>\"keep it simple\"</h6>\n                            </div>\n                            <div class=\"item-img\">\n                                <img src=\"@PROJECT_PATH@/assets/images/creator_5.929dec81-360x378.png\" alt=\"Art Desing\" data-slide-to=\"0\" data-bs-slide-to=\"0\">\n                            </div>\n                            <div class=\"img-bg\" mbr-if=\"showImgBg\"></div>\n                        </div>\n                    </div><div class=\"item features-image col-12 col-md-6 col-lg-4\">\n                        <div class=\"item-wrapper\">\n                            <div class=\"card-box\" mbr-if=\"showTitle || showSubtitle\">\n                                <h5 class=\"card-title align-left mbr-fonts-style align-center\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".card-title\">\n                                    CODE</h5>\n                                <h6 class=\"card-subtitle align-left mbr-fonts-style align-center\" mbr-theme-style=\"display-4\" mbr-if=\"showSubtitle\" data-app-selector=\".card-subtitle\"><b>\n                                    Head of Developing</b><br>\"life is