          "_customHTML": "<section data-bs-version=\"5.1\" class=\"pricing conferencem4_pricing\" group=\"Pricing Tables\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\">\n\n\t<mbr-parameters>\n\t\t<!-- Block parameters controls (Blue \"Gear\" panel) -->\n\t\t<header>Size</header>\n\t\t<input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n\t\t<input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"10\">\n\t\t<header>Show/Hide</header>\n\t\t<input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\" checked>\n\t\t<input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n\t\t<header>Card</header>\n\t\t<input type=\"checkbox\" title=\"Time\" name=\"showTime\" checked>\n\t\t<input type=\"checkbox\" title=\"Text\" name=\"showText\">\n\t\t<input type=\"checkbox\" title=\"Buttons\" name=\"showButtons\" checked>\n\t\t<select name=\"cardCount\" title=\"Card Count\">\n\t\t\t<option value=\"1\" selected>1</option>\n\t\t\t<option value=\"2\">2</option>\n\t\t\t<option value=\"3\">3</option>\n\t\t</select>\n\t\t<input type=\"color\" title=\"Color Card 1\" value=\"#fafafa\" name=\"colorItem1\">\n\t\t<input type=\"color\" title=\"Color Card 2\" value=\"#ffffff\" name=\"colorItem2\" condition=\"cardCount > 1\">\n\t\t<input type=\"color\" title=\"Color Card 3\" value=\"#fafafa\" name=\"colorItem3\" condition=\"cardCount > 2\">\n\t\t<header>Background</header>\n\t\t<fieldset type=\"background\" name=\"bg\" parallax>\n\t\t\t<input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/conference28-1920.jpg\">\n\t\t\t<input type=\"video\" title=\"Video\" value=\"https://www.youtube.com/watch?v=36YgDDJ7XSc\">\n\t\t\t<input type=\"color\" title=\"Color\" value=\"#fafafa\" selected>\n\t\t</fieldset>\n\t\t<input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\">\n\t\t<input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#149dcc\" condition=\"overlay && bg.type !== 'color'\">\n\t\t<input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.6\" condition=\"overlay && bg.type !== 'color'\">\n\t\t<!-- End block parameters -->\n\t</mbr-parameters>\n\n\t<div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n\t</div>\n\n\t<div class=\"container\">\n\t\t<div class=\"row\">\n\t\t\t<div class=\"col-lg-12 content__block align-center\">\n\t\t\t\t<h3 class=\"mbr-section-subtitle mbr-semibold mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-subtitle\"><b>YRALINK OFFER</b>&nbsp;</h3>\n\t\t\t\t<h2 class=\"mbr-section-title mbr-bold mbr-fonts-style\" mbr-theme-style=\"display-1\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title\">Get Your Personal Blog in One Day</h2>\n\t\t\t</div>\n\t\t</div>\n\t\t<div class=\"row wrap justify-content-center\">\n\t\t\t<div class=\"col-lg-4 col-md-6\">\n\t\t\t\t<div class=\"wrap__card item1\">\n\t\t\t\t\t<h4 class=\"card__title mbr-bold mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".card__title\">Personal Blog</h4>\n\t\t\t\t\t<h3 class=\"card__price mbr-bold mbr-fonts-style\" mbr-theme-style=\"display-1\" data-app-selector=\".card__price\">4999</h3>\n\t\t\t\t\t<div class=\"time__block\">\n\t\t\t\t\t\t<p class=\"card__time mbr-medium mbr-fonts-style\" mbr-if=\"showTime\" mbr-theme-style=\"display-4\" data-app-selector=\".card__time\">1 Website Blog&nbsp;<br></p>\n\t\t\t\t\t</div>\n\t\t\t\t\t<p class=\"mbr-text mbr-medium mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".mbr-text, .mbr-section-btn, .price__list_check, .price__list_close, .time__block\">You will get your&nbsp;<b>Personal Blog</b> in <b>1 day</b>