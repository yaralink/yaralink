        {
          "_name": "afooter1",
          "_sourceTheme": "mediam5",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"footer1 mediam5\" group=\"Footers\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" global once=\"footers\" mbr-class=\"{\n    'mbr-parallax-background': bg.parallax}\">\n\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"3\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"1\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Main Links\" name=\"showMainLinks\" checked>\n        <input type=\"checkbox\" title=\"Links\" name=\"showLinks\" checked>\n        <input type=\"checkbox\" title=\"Contacts 1\" name=\"showContacts1\" checked>\n        <input type=\"checkbox\" title=\"Contacts 2\" name=\"showContacts2\" checked>\n        <input type=\"range\" inline title=\"Contacts 1 Count\" name=\"contacts1Count\" min=\"1\" max=\"2\" step=\"1\" value=\"2\" condition=\"showContacts1\">\n        <input type=\"range\" inline title=\"Contacts 2 Count\" name=\"contacts2Count\" min=\"1\" max=\"2\" step=\"1\" value=\"2\" condition=\"showContacts2\">\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked condition=\"showContacts1 || showContacts2\">\n        <input type=\"checkbox\" title=\"Icons\" name=\"showIcon\" checked condition=\"showContacts1 || showContacts2\">\n        <input type=\"color\" title=\"Icon Color\" name=\"iconColor\" value=\"#f4b400\" selected condition=\"showIcon && (showContacts1 || showContacts2)\">\n        <input type=\"checkbox\" title=\"Text\" name=\"showText\" checked>\n        <input type=\"checkbox\" title=\"Copyright\" name=\"showCopyright\" checked>\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@ADDONS_PATH@/mediam5/components/_images/background3.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#1a2026\" selected>\n            <input type=\"video\" title=\"Video\" value=\"https://vimeo.com/428046504\">\n        </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"@ADDONS_PATH@/mediam5/components/_images/background2.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#181817\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\">\n            <div class=\"col-12\">\n                <div class=\"main-container\">\n                    <div class=\"main-links grid-box\" mbr-if=\"showMainLinks\">\n                        <div class=\"item-wrap\">\n                            <ul class=\"list main-list mbr-fonts-style\" mbr-theme-style=\"display-1\" data-app-selector=\".main-list\" data-multiline=\"true\" data-app-edit=\"full\" mbr-article>\n                                <li class=\"mbr-text item-wrap\" data-app-selector=\".main-list\"><span style=\"color: inherit; font-weight: 900; font-size: 4rem;\">ABOUT</span></li><li class=\"mbr-text item-wrap\" data-app-selector=\".main-list\"><span style=\"color: inherit; font-weight: 900; font-size: 4rem;\">SITES</span></li><li class=\"mbr-text item-wrap\" data-app-selector=\".main-list\"><span style=\"color: inherit; font-weight: 900; font-size: 4rem;\">PLAN</span></li><li class=\"mbr-text item-wrap\" data-app-selector=\".main-list\"><span style=\"color: inherit; font-weight: 900; font-size: 4rem;\">SUPPORT</span></li>\n                            </ul>\n                        </div>\n                    </div>\n                    <div class=\"second-links grid-box\" mbr-if=\"showLinks\">\n                        <div class=\"item-wrap\">\n                            <ul class=\"list second-list mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".second-list\" data-multiline=\"true\" data-app-edit=\"full\" mbr-article>\n                                <li class=\"mbr-text item-wrap\" data-app-selector=\".second-list\">Personal Blog</li><li class=\"mbr-text item-wrap\" data-app-selector=\".second-list\">Company Site</li><li class=\"mbr-text item-wrap\" data-app-selector=\".second-list\">Online Store</li><li class=\"mbr-text item-wrap\" data-app-selector=\".second-list\">Chat Bot</li><li class=\"mbr-text item-wrap\" data-app-selector=\".second-list\">Form Download</li><li class=\"mbr-text item-wrap\" data-app-selector=\".second-list\">FAQ</li>\n                            </ul>\n                        </div>\n                    </div>\n\n                    <div class=\"address-item address-item-1 grid-box\" mbr-if=\"showContacts1\">\n                        <div class=\"item-wrap\">\n                            <h5 class=\"mbr-section-subtitle mbr-fonts-style\" data-app-selector=\".mbr-section-subtitle\" mbr-theme-style=\"display-4\" mbr-if=\"showTitle\">\n                               <b>Instant Reply</b></h5>\n                            <div class=\"card-text-container\">\n                                <div class=\"iconfont-wrapper\" mbr-if=\"showIcon\">\n                                    <span mbr-icon class=\"mbr-iconfont socicon-telegram socicon\" style=\"color: rgb(66, 133, 244); fill: rgb(66, 133, 244);\"></span>\n                                </div>\n                                <p class=\"card-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card-text\">\n                                    <a href=\"tg://resolve?domain=yaralink\" class=\"text-white\">Telegram</a></p>\n                            </div>\n                            <div class=\"card-text-container\" mbr-if=\"contacts1Count >1\">\n                                <div class=\"iconfont-wrapper\" mbr-if=\"showIcon\">\n                                    <span mbr-icon class=\"mbr-iconfont socicon-whatsapp socicon\" style=\"color: rgb(15, 157, 88); fill: rgb(15, 157, 88);\"></span>\n                                </div>\n                                <p class=\"card-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card-text\">\n                                    <a href=\"https://wa.me/9779818008981\" class=\"text-white\">WhatsApp</a></p>\n                            </div>\n                        </div>\n                    </div>\n                    <div class=\"address-item address-item-2 grid-box\" mbr-if=\"showContacts2\">\n                        <div class=\"item-wrap\">\n                            <h5 class=\"mbr-section-subtitle mbr-fonts-style\" data-app-selector=\".mbr-section-subtitle\" mbr-theme-style=\"display-4\" mbr-if=\"showTitle\">\n                                <b>Contact</b></h5>\n                            <div class=\"card-text-container\">\n                                <div class=\"iconfont-wrapper\" mbr-if=\"showIcon\">\n                                    <span mbr-icon class=\"mbr-iconfont mdi-communication-email\" style=\"color: rgb(244, 180, 0); fill: rgb(244, 180, 0);\"></span>\n                                </div>\n                                <p class=\"card-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card-text\">\n                                    1@yaralink.com</p>\n                            </div>\n                            <div class=\"card-text-container\" mbr-if=\"contacts2Count >1\">\n                                <div class=\"iconfont-wrapper\" mbr-if=\"showIcon\">\n                                    <span mbr-icon class=\"mbr-iconfont mdi-communication-phone\" style=\"color: rgb(219, 68, 55); fill: rgb(219, 68, 55);\"></span>\n                                </div>\n                                <p class=\"card-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card-text\">\n                                    +977 981 800 8981</p>\n                            </div>\n                        </div>\n                    </div>\n                    <p class=\"footer-text mb-0 mbr-fonts-style align-left\" mbr-theme-style=\"display-7\" data-app-selector=\".footer-text\" data-app-placeholder=\"Type Text\" mbr-if=\"showText\"><b>Yaralink</b> - we create affordable website in 3 simple steps. Get a Personal Blog, Company Site, Online Store or Chat Bot with training to manage your site.</p>\n                    <div class=\"copyright-item grid-box\" mbr-if=\"showCopyright\">\n                        <p class=\"mb-0 mbr-fonts-style copyright align-right\" mbr-theme-style=\"display-7\" data-app-selector=\".copyright\" data-app-placeholder=\"Type Text\">2023  Yaralink Development. All Rights Reserved.</p>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
